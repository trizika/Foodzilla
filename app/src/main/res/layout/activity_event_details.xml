<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EventDetails">

    <TextView
        android:id="@+id/textVieweventdetailsedetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="165dp"
        android:layout_marginTop="61dp"
        android:drawableTint="#FFFFFF"
        android:text="Event Details"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textVieweventnameedetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="48dp"
        android:text="Event Name:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVieweventdetailsedetails" />

    <TextView
        android:id="@+id/textVieweventdateedetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="64dp"
        android:text="Event Date:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVieweventnameedetails" />

    <TextView
        android:id="@+id/textVieweventtimeedetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="80dp"
        android:text="Event Time:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVieweventdateedetails" />

    <TextView
        android:id="@+id/textVieweventlocationedetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="72dp"
        android:text="Event Location:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVieweventtimeedetails" />

    <TextView
        android:id="@+id/textVieweventinfoedetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="96dp"
        android:text="Event Info:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVieweventlocationedetails" />

    <TextView
        android:id="@+id/textView2eventnameedetails"
        android:layout_width="124dp"
        android:layout_height="29dp"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="124dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textVieweventnameedetails"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2locationedetails"
        android:layout_width="107dp"
        android:layout_height="26dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="75dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toEndOf="@+id/textVieweventlocationedetails"
        app:layout_constraintTop_toBottomOf="@+id/textView2eventtimeedetails" />

    <TextView
        android:id="@+id/textView2eventdateedetails"
        android:layout_width="112dp"
        android:layout_height="25dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="128dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textVieweventdateedetails"
        app:layout_constraintTop_toBottomOf="@+id/textView2eventnameedetails" />

    <TextView
        android:id="@+id/textView2infoeventdetails"
        android:layout_width="103dp"
        android:layout_height="25dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="75dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.579"
        app:layout_constraintStart_toEndOf="@+id/textVieweventinfoedetails"
        app:layout_constraintTop_toBottomOf="@+id/textView2locationedetails" />

    <TextView
        android:id="@+id/textView2eventtimeedetails"
        android:layout_width="114dp"
        android:layout_height="34dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="75dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.646"
        app:layout_constraintStart_toEndOf="@+id/textVieweventtimeedetails"
        app:layout_constraintTop_toBottomOf="@+id/textView2eventdateedetails" />

</androidx.constraintlayout.widget.ConstraintLayout>